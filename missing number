import java.util.*;
import java.lang.*;
import java.io.*;

class GFG {
	public static void main (String[] args) {
		Scanner sc=new Scanner(System.in);
		int T=sc.nextInt();
		int sum=0;
		int a[]=new int[10000];
		for(int i=0;i<T;i++){
		    int n=sc.nextInt();
		    sum=n*(n+1)/2;
		    int sum1=0;
		    for(int j=0;j<n-1;j++){
		        a[i]=sc.nextInt();
		        sum1=sum1+a[i];
		    }
		    System.out.println(sum-sum1);
		}
		}
	}
